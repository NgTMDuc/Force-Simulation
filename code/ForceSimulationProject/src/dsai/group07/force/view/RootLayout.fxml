<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="rootLayout" maxHeight="1000.0" maxWidth="2000.0" minHeight="400.0" minWidth="400.0" prefHeight="600.0" prefWidth="970.8" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dsai.group07.force.controller.ForceSimulationAppController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" percentHeight="70.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <AnchorPane>
         <children>
            <Button fx:id="pauseButton" disable="true" mnemonicParsing="false" onAction="#pauseButtonPressed" text="&gt;" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="100.0" />
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetButtonPressed" text="Reset" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="25.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
